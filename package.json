{
  "name": "aft-testrail",
  "version": "4.0.0",
  "description": "Automated Functional Testing (AFT) package supporting TestRail integration for test execution control and logging",
  "main": "./dist/src/index/js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "testrail"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aft-web-services": "^4.0.0",
    "es6-promise": "^4.2.8",
    "ts-simple-nameof": "^1.3.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "@types/node": "^12.19.15",
    "jasmine": "^3.6.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
